<% @compats.each do |gemmy, rails_release| %>
  $("[data-gemmy='<%= gemmy.to_global_id %>'][data-rails-release='<%= rails_release.to_global_id %>']").replaceWith(
    "<%= j render 'compats/table_cell', gemmy: gemmy, rails_release: rails_release %>"
  )
<% end %>
<% if @compats.any? %>
  $('[data-toggle="tooltip"]').tooltip()
<% end %>
